{
  "name": "ugamers-api",
  "version": "0.0.1",
  "repository": "https://github.com/valdineifer/ugamers-api.git",
  "description": "",
  "author": "Valdinei Ferreira <valdineifer.dev@outlook.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,libs,test}/**/*.ts\"",
    "lint:fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/configs/typeorm.config.ts",
    "db:fresh": "npm run typeorm schema:drop && npm run typeorm migration:run",
    "db:seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "db:seed:generate": "ts-node src/commands/generate-seed.ts",
    "db:seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n src/configs/typeorm.config.ts",
    "db:fresh:seed": "npm run db:fresh && npm run db:seed:run"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.14",
    "@nestjs/core": "^7.6.14",
    "@nestjs/graphql": "^7.9.11",
    "@nestjs/platform-express": "^7.6.14",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.21.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "connect-redis": "^5.1.0",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express-session": "^1.17.1",
    "faker": "^5.4.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "helmet": "^4.4.1",
    "ioredis": "^4.23.0",
    "nest-winston": "^1.4.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.14",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.1.7",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
